        android:icon="@mipmap/ic_launcher"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:hardwareAccelerated="false"
        android:largeHeap="true">
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".HomeActivity"
                  android:parentActivityName=".MainActivity"
                  android:label="BuSezon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".shoppingCart"
            android:parentActivityName=".HomeActivity"
            android:label="Shopping Cart">
        </activity>
        <activity android:name=".ProductDesc"
            android:label="">

        </activity>
        <activity
            android:name=".ClothingPage"
            android:label="Clothing" />
        <activity
            android:name=".SellHomepage"
            android:label="Seller Home"/>
        <activity
            android:name=".ProductDescription"
            android:label="Product Description"/>
    </application>

</manifest>
